{
    "name": "michelmelo/laravel-adminer",
    "description": "Laravel Adminer DB Manager",
    "keywords": [
        "laravel8",
        "laravel9",
        "laravel10",
        "adminer",
        "database",
        "management",
        "mysql",
        "sqlite"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Michel Melo",
        "email": "eu@michelmelo.pt"
    }],
    "require": {
        "php": ">=5.4.0",
        "illuminate/contracts": "~8.0|~9.0|~10.0",
        "illuminate/routing": "~8.0|~9.0|~10.0",
        "illuminate/support": "~8.0|~9.0|~10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14"
    },
    "autoload": {
        "psr-4": {
            "MichelMelo\\Adminer\\": "src/"
        }
    },
    "scripts": {
        "fixer": [
            "php-cs-fixer fix"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichelMelo\\Adminer\\ServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}